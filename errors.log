SELECT author.author_id AS author_author_id, author.first_name AS author_first_name, author.last_name AS author_last_name 
FROM author
[Isaac Asimov, Pearl Buck, Tom Clancy, Stephen King, John Le Carre, Alex Michaelides, Carol Shaben]

SELECT book.book_id AS book_book_id, book.author_id AS book_author_id, book.title AS book_title 
FROM book
[Dead Zone,
 Foundation,
 Into The Abyss,
 It,
 Patriot Games,
 The Good Earth,
 The Hunt For Red October,
 The Shining,
 The Silent Patient,
 Tinker, Tailor, Solider, Spy: A George Smiley Novel]

SELECT publisher.publisher_id AS publisher_publisher_id, publisher.name AS publisher_name 
FROM publisher
[Random House, Simon & Schuster, Berkley, Penguin Random House]
